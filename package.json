{
    "name": "@aitianyu.cn/tianyu-store",
    "version": "0.0.5",
    "description": "tianyu storage for nodejs.",
    "main": "./dist/lib/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": "./dist/lib/index.js"
    },
    "files": [
        "dist"
    ],
    "type": "commonjs",
    "scripts": {
        "start": "ts-node src/index.ts",
        "build": "npm run before-build && npm run compiler && npm run copy-lib && npm run copy-types && npm run after-build",
        "compiler": "tsc && tsc-alias",
        "copy-lib": "copyfiles -u 1 \"build/src/**/*.js\" \"build/index.js\" dist/lib",
        "copy-types": "copyfiles -u 1 \"build/src/**/*.d.ts\" \"build/index.d.ts\" dist/types",
        "before-build": "rimraf ./dist ./build",
        "after-build": "rimraf ./build",
        "test": "jest --no-cache --verbose",
        "test-all": "jest --no-cache --verbose --coverage",
        "test-coverage": "jest --no-cache --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aitianyu-cn/tianyu-store"
    },
    "keywords": [
        "aitianyu.cn",
        "store"
    ],
    "author": "dev@aitianyu.cn",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/aitianyu-cn/tianyu-store/issues"
    },
    "homepage": "https://github.com/aitianyu-cn/tianyu-store#readme",
    "devDependencies": {
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.13",
        "copyfiles": "^2.4.1",
        "jest": "^29.3.1",
        "jest-html-reporters": "^3.0.11",
        "rimraf": "^3.0.2",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.2",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@aitianyu.cn/types": "latest"
    }
}
